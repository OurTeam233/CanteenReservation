.top-tabBar-wrapper {
  margin-bottom: 300rpx;
}
.top-tabBar-wrapper .card-wrapper {
  margin: 0 40rpx;
}
.top-tabBar-wrapper .confessionWall-wrapper {
  margin: 0 40rpx;
}
.top-tabBar-wrapper .search-search .search-title {
  display: flex;
  align-items: center;
  font-size: 21rpx;
  background-color: #fff;
  margin: 0;
}
.top-tabBar-wrapper .search-search .search-title .search-button-find {
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
  margin: 0 20rpx 0 20rpx;
}
.top-tabBar-wrapper .search-search .search-title .search-button-find .search-button-find-text {
  margin-right: 8rpx;
}
.top-tabBar-wrapper .search-search .search-title .search-button-find .search-button-find-icon {
  font-size: 45rpx;
}
.top-tabBar-wrapper .search-search .search-title .search-button-search {
  height: 100%;
  display: flex;
  justify-content: center;
  align-items: center;
}
.top-tabBar-wrapper .search-search .search-title .search-button-search .search-button-search-text {
  margin-right: 8rpx;
}
.top-tabBar-wrapper .search-search .search-title .search-button-search .search-button-search-icon {
  font-size: 45rpx;
}
.top-tabBar-wrapper .search-search .search-title .search-wrapper {
  flex: 1;
}
.top-tabBar-wrapper .search-search .search-title .active-search {
  color: #ED4450;
}
.top-tabBar-wrapper .search-search .search-card-wrapper {
  padding: 20rpx 20rpx;
}
.top-tabBar-wrapper .search-search .search-card-wrapper .search-card {
  background-color: #fff;
  border-radius: 20rpx;
  margin-bottom: 20rpx;
}
